{
  "name": "electric-trumpet",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start-hot": "electron -r babel-register ./main.js",
    "hot-server": "node -r babel-register dev.js",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "concurrently": "^2.0.0",
    "express": "^4.13.4",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
